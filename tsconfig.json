{
  "extends": "./node_modules/electron-webpack/tsconfig-base.json",
  "compilerOptions": {
    "paths": {
      "@/*": [
        "renderer/*"
      ],
      "common/*": [
        "common/*"
      ]
    },
    "skipLibCheck": true,
    "noImplicitAny": false,
    "baseUrl": "source/",
    "jsx": "react",
    "typeRoots": [
      "typings",
      "node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
